<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<description>Wiring the dependencies for the Facility Application.</description>

	<bean id="use" class="use.UseImpl" scope="prototype"></bean>

	<bean id="facilityDetail" class="facility.FacilityDetailImpl" scope="prototype"></bean>

	<bean id="maintenance" class="maintenance.MaintenanceImpl" scope="prototype"></bean>

	<bean id="facility" class="facility.FacilityImpl" scope="prototype">
		<property name="facility">
			<list/>
		</property>
	</bean>

	<bean id="demoTools" class="DemoTools" scope="prototype"></bean>


</beans>
